<Window x:Class="ApiTester.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ApiTester"
        mc:Ignorable="d"
        Title="MainWindow" Height="297.922" Width="432.271">
    <Grid>
        <Button x:Name="CreateButton" Content="Create Lobby" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="110" Click="CreateButton_Click"/>
        <Button x:Name="DeleteButton" Content="Delete Lobby" HorizontalAlignment="Left" Margin="10,60,0,0" VerticalAlignment="Top" Width="110" Click="DeleteButton_Click"/>
        <ListBox x:Name="LobbyListBox" HorizontalAlignment="Left" Height="253" Margin="137,10,0,0" VerticalAlignment="Top" Width="281" SelectionChanged="LobbyListBox_SelectionChanged"/>
        <Button x:Name="RefreshButton" Content="Refresh" HorizontalAlignment="Left" Margin="10,85,0,0" VerticalAlignment="Top" Width="110
                " Click="RefreshButton_Click"/>
        <Button x:Name="ShowIPButton" Content="Show IP" HorizontalAlignment="Left" Margin="10,35,0,0" VerticalAlignment="Top" Width="110" Click="ShowIPButton_Click"/>

    </Grid>
</Window>
